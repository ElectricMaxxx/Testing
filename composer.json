{
    "name": "symfony-cmf/testing",
    "license": "MIT",
    "description": "Component providing tools for writing tests with Symfony.",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/MenuBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/symfony": "~2.1",
        "symfony/monolog-bundle": "~2.1",
        "doctrine/common": "~2.4",
        "doctrine/doctrine-bundle": "1.*",
        "doctrine/phpcr-odm": "1.*",
        "doctrine/phpcr-bundle": "1.*",
        "doctrine/data-fixtures": "1.0.*",
        "jackalope/jackalope": "1.*",
        "jackalope/jackalope-doctrine-dbal": "1.*"
    },
    "autoload": {
        "psr-0": { "Symfony\\Cmf\\Component\\Testing": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
